{
    "name": "roryy/trafficjam",
    "description": "Queue Library for RabbitMQ or SQS",
    "type":"library",
    "license": "MIT",
    "require":{
        "php": "^7.1",
        "php-amqplib/php-amqplib": "~2.12",
        "psr/log": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Trafficjam\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Trafficjam\\Test\\": "tests"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5",
        "friendsofphp/php-cs-fixer": "^2.17"
    }
}
